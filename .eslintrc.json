{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-undef": 0,
		"indent": [0, "tab"],
		"no-tabs": 0,
		"key-spacing": 0,
		"comma-dangle": 0,
		"no-mixed-spaces-and-tabs": 1,
		"no-unused-vars": 1,
		"react/prop-types": "off",
		"no-debugger": "off"
	}
}
